{
  "name": "restaurants-mfe",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "loki:install": "cd ./node_modules/@loki/root && yarn --frozen-lockfile --non-interactive --silent --ignore-scripts",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "dev": "webpack serve --env port=3012",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\" --fix",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:report": "yarn run test:ui:json && yarn run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "storybook": "storybook dev -p 6006 --config-dir ./config/storybook",
    "storybook:build": "storybook build --config-dir ./config/storybook"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "7.13.8",
    "@babel/preset-typescript": "^7.21.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@storybook/addon-actions": "^7.0.6",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/addon-mdx-gfm": "^7.0.6",
    "@storybook/cli": "^7.0.6",
    "@storybook/node-logger": "^7.0.6",
    "@storybook/preset-create-react-app": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/react-webpack5": "^7.0.6",
    "@storybook/testing-library": "^0.1.0",
    "@svgr/webpack": "^7.0.0",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/compose-function": "^0.0.30",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.35",
